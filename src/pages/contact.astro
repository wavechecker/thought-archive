---
import { getCollection } from "astro:content";

// Find the entry with slug "contact" in the "pages" collection
const entry = (await getCollection("pages")).find((e) => e.slug === "contact");

// Render function for markdown entries
const rendered = entry ? await entry.render() : null;
const Content = rendered ? rendered.Content : null;
---

{Content ? <Content /> : <p>Contact page not found.</p>}
